 => pressure),\r\n        fill: true,\r\n        borderColor: 'rgba(75,192,192,1)',\r\n        backgroundColor: 'rgba(75,192,192,0.4)',\r\n      },\r\n    ],\r\n  };\r\n\r\n  return (\r\n    <div className=\"chart-container\">\r\n      <Carousel\r\n        cols={1}\r\n        rows={1}\r\n        gap={5}\r\n        loop={false}\r\n        hideArrow={false}\r\n        showDots\r\n      >\r\n        <Carousel.Item>\r\n          <Line data={chartDataOne} options={options} />\r\n        </Carousel.Item>\r\n        <Carousel.Item>\r\n          <Line data={chartDataTwo} options={options} />\r\n        </Carousel.Item>\r\n        <Carousel.Item>\r\n          <Line data={chartDataThree} options={options} />\r\n        </Carousel.Item>\r\n      </Carousel>\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default Charts","C:\\Users\\DELL\\OneDrive\\Desktop\\prep-project\\prep-project-22.P3.1\\src\\hooks\\useLocation.js",[],"C:\\Users\\DELL\\OneDrive\\Desktop\\prep-project\\prep-project-22.P3.1\\src\\hooks\\useFetchCity.js",[],"C:\\Users\\DELL\\OneDrive\\Desktop\\prep-project\\prep-project-22.P3.1\\src\\components\\backgroundArray\\backgroundArray.js",[],"C:\\Users\\DELL\\OneDrive\\Desktop\\prep-project\\prep-project-22.P3.1\\src\\components\\backgroundArray\\weatherCard.js",[],"C:\\Users\\DELL\\OneDrive\\Desktop\\prep-project\\prep-project-22.P3.1\\src\\components\\Navbar\\navbar.js",[],"C:\\Users\\DELL\\OneDrive\\Desktop\\prep-project\\prep-project-22.P3.1\\src\\components\\Navbar\\Search.jsx",[],"C:\\Users\\DELL\\OneDrive\\Desktop\\prep-project\\prep-project-22.P3.1\\src\\components\\Navbar\\cities.js",[],{"ruleId":"47","severity":1,"message":"48","line":72,"column":1,"nodeType":null,"messageId":"49","endLine":72,"endColumn":7,"fix":"50"},{"ruleId":"47","severity":1,"message":"51","line":74,"column":1,"nodeType":null,"messageId":"49","endLine":74,"endColumn":3,"fix":"52"},{"ruleId":"47","severity":1,"message":"53","line":121,"column":22,"nodeType":null,"messageId":"54","endLine":121